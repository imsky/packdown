{
  "name": "packdown",
  "version": "0.0.0",
  "formatVersion": 1,
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "mocha test/**/*-test.js",
    "nearley": "nearleyc lib/packdown.ne -o lib/grammar.js",
    "browserify": "browserify -s Packdown index.js -o packdown.js",
    "uglify": "uglifyjs packdown.js -o packdown.min.js",
    "format": "jsfmt --write -v lib",
    "lint": "eslint lib/**/*.js",
    "cleanup": "",
    "quality": "npm test && npm run format && npm run lint",
    "banner": "cat packdown-banner.txt packdown.js > packdown-tmp.js && cat packdown-banner.txt packdown.min.js > packdown.min-tmp.js && mv packdown-tmp.js packdown.js && mv packdown.min-tmp.js packdown.min.js",
    "prebuild": "npm run quality && node extras/packdown-metadata && npm run nearley",
    "build": "npm run browserify",
    "postbuild": "npm run uglify && npm run banner",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imsky/packdown.git"
  },
  "keywords": [
    "markdown",
    "pack",
    "file",
    "collection",
    "concatenate"
  ],
  "author": "Ivan Malopinsky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imsky/packdown/issues"
  },
  "homepage": "https://github.com/imsky/packdown#readme",
  "dependencies": {
    "base-64": "^0.1.0",
    "nearley": "^1.6.2",
    "utf8": "^2.1.1"
  },
  "devDependencies": {
    "bluebird": "^2.10.0",
    "browserify": "^11.1.0",
    "chai": "^3.3.0",
    "chai-as-promised": "^5.1.0",
    "del": "^2.0.2",
    "diff": "^2.1.1",
    "eslint": "^1.5.0",
    "jsfmt": "^0.5.0",
    "jsonfile": "^2.2.2",
    "mocha": "^2.3.2",
    "uglify-js": "^2.4.24",
    "walk": "^2.3.9"
  }
}
