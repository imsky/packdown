{
  "name": "packdownjs",
  "version": "0.9.0",
  "author": "Ivan Malopinsky",
  "license": "MIT",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha './test/*-test.js'",
    "browserify": "browserify -s Packdown index.js -o dist/packdown.js",
    "uglify": "uglifyjs dist/packdown.js -o dist/packdown.min.js",
    "format": "jsfmt --write -v lib",
    "lint": "eslint index.js",
    "quality": "npm run format && npm run lint && npm run test",
    "build": "npm run quality && npm run browserify",
    "postbuild": "npm run uglify && node scripts/version"
  },
  "keywords": [
    "markdown",
    "pack",
    "file",
    "collection",
    "template",
    "boilerplate",
    "scaffold",
    "gist",
    "fiddle"
  ],
  "bugs": {
    "url": "https://github.com/imsky/packdown/issues"
  },
  "homepage": "https://github.com/imsky/packdown",
  "dependencies": {},
  "devDependencies": {
    "browserify": "^11.1.0",
    "chai": "4.2.0",
    "eslint": "7.11.0",
    "jsfmt": "^0.5.3",
    "mocha": "8.2.0",
    "nyc": "15.1.0",
    "reify": "0.20.12",
    "shelljs": "^0.5.3",
    "uglify-js": "^2.4.24"
  },
  "nyc": {
    "check-coverage": true,
    "branches": 100
  }
}
